<!-- applicationContext.xml -->
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd"> 
    
                   <!--Setter Injection-->
    
    <bean id="battery" class="com.example.demo.Battery">
        <property name="productId" value="B001"/>
        <property name="productName" value="Rechargeable Battery"/>
        <property name="price" value="15.99"/>
        <property name="Rechargeable" value="2000"/>
    </bean>

    <bean id="disc" class="com.example.demo.Disc">
        <property name="productId" value="D001"/>
        <property name="productName" value="DVD"/>
        <property name="price" value="5.99"/>
        <property name="Capacity" value="DVD-R"/>
    </bean>
</beans>